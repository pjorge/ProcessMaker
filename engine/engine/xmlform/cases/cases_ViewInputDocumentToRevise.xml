<?xml version="1.0" encoding="UTF-8"?>
<dynaForm name="cases_InputDocument" type="xmlform" width="400px" labelWidth="100px">
  <INP_DOC_UID type="private"/>
  <POSITION type="private"/>
  <APP_UID type="private"/>
  <PRO_UID type="private"/>
  <DEL_INDEX type="private"/>
  <APP_DOC_TITLE type="caption">
    <en>Title</en>
    <pt><![CDATA[Título]]></pt>
  </APP_DOC_TITLE>
  <APP_DOC_COMMENT type="caption">
    <en>Comment</en>
    <pt><![CDATA[Comentário]]></pt>
  </APP_DOC_COMMENT>
  <APP_DOC_TYPE type="text" colWidth="80" titleAlign="left" align="left" mode="view">
    <en>Type</en>
    <pt><![CDATA[Tipo]]></pt>
  </APP_DOC_TYPE>
  <INP_DOC_ORIGINAL type="dropdown" colWidth="80" titleAlign="left" align="left" mode="view">
    <en>Format<option name="ORIGINAL">Original</option><option name="COPYLEGAL">Legal Copy</option><option name="COPY">Copy</option></en>
    <pt><![CDATA[Formato]]><option name="ORIGINAL"><![CDATA[Original]]></option><option name="COPYLEGAL"><![CDATA[Cópia autenticada]]></option><option name="COPY"><![CDATA[Copia]]></option></pt>
  </INP_DOC_ORIGINAL>
  <APP_DOC_CREATE_DATE type="date" mode="view">
    <en>Created Date</en>
    <pt><![CDATA[Data de criação]]></pt>
  </APP_DOC_CREATE_DATE>
  <CREATOR type="text" mode="view">
    <en>Creator</en>
    <pt><![CDATA[Criador]]></pt>
  </CREATOR>
  <VIEW type="phpVariable"/>
  <FILE type="phpVariable"/>
  <APP_DOC_FILENAME type="link" value="@#VIEW" link="@#FILE" target="_blank">
    <en>File</en>
    <pt><![CDATA[Arquivo]]></pt>
  </APP_DOC_FILENAME>
  <BACK type="button" onclick="back();">
    <en>Back</en>
    <pt><![CDATA[Voltar]]></pt>
  </BACK>
  <JS type="javascript">


var back = function()
{
	javascript:history.go(-1);
};

</JS>
</dynaForm>
